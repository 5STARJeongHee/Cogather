<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.cogather.studygroup.model.StudyGroupDAO">
	<select id="getStudyGroupBySGID" resultType="com.project.cogather.studygroup.model.StudyGroupDTO">
		SELECT sg_id, sg_name, sg_info, sg_max, sg_regdate, sg_tag, kko_url
		FROM studygroup
		where sg_id = #{sg_id}
	</select>
</mapper>